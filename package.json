    "prop-types": "^15.5.8",
    "react-display-name": "^0.2.0",
    "react-prop-types-element-of-type": "^2.2.0",
    "scriptjs": "^2.5.8",
    "warning": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^15.5.4",
    "react-dom": "1^5.5.4"
  }
}
